{
  "name": "Partner Integration API",
  "path": "/api/partner",
  "state": "published",
  "version": "1.0",
  "organization": "External Partners",
  "apiSpecification": {
    "resource": "../OAS/partner-api-openapi.json"
  },
  "vhost": "ec2-18-207-108-191.compute-1.amazonaws.com:8065",
  "backendBasepath": "http://partner-backend:8080",
  "inboundProfiles": {
    "_default": {
      "monitorAPI": true,
      "queryStringPassThrough": false,
      "monitorSubject": "authentication.subject.id",
      "securityProfile": "partner-auth",
      "corsProfile": "_default"
    }
  },
  "securityProfiles": {
    "partner-auth": {
      "devices": [
        {
          "name": "Partner Token",
          "type": "apiKey",
          "order": 1,
          "properties": {
            "apiKeyFieldName": "X-Partner-Token",
            "apiKeyLocation": "HEADER"
          }
        }
      ]
    }
  },
  "clientOrganizations": [
    "External Partners"
  ],
  "applications": [
    {
      "name": "Partner App",
      "enabled": true,
      "state": "approved",
      "permissions": [],
      "appScopes": []
    }
  ]
} 