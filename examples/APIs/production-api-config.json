{
  "name": "Production API",
  "path": "/api/production",
  "state": "published",
  "version": "1.0",
  "organization": "Production",
  "apiSpecification": {
    "resource": "../OAS/production-api-openapi.json"
  },
  "vhost": "ec2-18-207-108-191.compute-1.amazonaws.com:8065",
  "backendBasepath": "http://production-backend:8080",
  "inboundProfiles": {
    "_default": {
      "monitorAPI": true,
      "queryStringPassThrough": false,
      "monitorSubject": "authentication.subject.id",
      "securityProfile": "production-oauth",
      "corsProfile": "_default"
    }
  },
  "securityProfiles": {
    "production-oauth": {
      "devices": [
        {
          "name": "OAuth",
          "type": "oauth",
          "order": 1,
          "properties": {
            "tokenStore": "TokenInfoPolicy",
            "accessTokenLocation": "HEADER",
            "accessTokenLocationName": "Authorization",
            "accessTokenLocationValue": "Bearer "
          }
        }
      ]
    }
  },
  "clientOrganizations": [
    "Production"
  ],
  "applications": [
    {
      "name": "Production App",
      "enabled": true,
      "state": "approved",
      "permissions": [],
      "appScopes": []
    }
  ]
} 